
<html>
<head>
    <% include common/head %>
    <!--additional JS and css-->
</head>
<body>
<header id="header" class="header full-width transparent fixed header-small clearfix">
    <% include common/header %>
</header>

<section id="content_body">
    <div class="space-box">
        <div class="container-fluid">
            <form class="defaultform row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="register-widget clearfix">
                        <div class="space-top"></div>
                        <div class="widget-title">
                            <h3>Reset Password</h3>
                            <hr>
                        </div><!-- end title -->
                        <div class="row">
                            <div class="form-group col-lg-12">
                                <label>Password</label>
                                <input type="password" id="password" class="form-control">
                            </div>
                            <div class="form-group col-lg-12">
                                <label>Confirm Password</label>
                                <input type="password" id="cnf_password" class="form-control">
                            </div>
                            <div class="col-sm-12">
                                <button type="button" id="btnResetPassword" class="btn btn-default">Reset Password</button>
                                <hr />
                            </div>
                        </div>
                    </div>
                </div>
            </form><!-- end row -->
        </div><!-- end container -->
    </div>

</section>

<footer id="footer">
    <% include common/footer %>
</footer>

</body>
</html>