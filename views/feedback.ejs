<!Doctype HTML>
<html>
<head>
    <% include common/head %>
    <!--additional JS and css-->
</head>
<body>
	<header id="header">
	    <% include common/header %>
	</header>
	<section id="content_body">
		<div class="feedback-body ">
			<div class="container">
				<div class="row feedback-heading">
					<div class="col-lg-12 wow fadeInDown" data-wow-duration="3s" data-wow-delay="0.4s">
						<h1 class="text-center"> Feedback</h1>
						<p class="text-center">Thank you for your time to let us know what you think about us</p>
					</div>
				</div>
			</div>
			<div class="feedback-contact">
				<div class="container">
					<div class="row">
						<div class="col-lg-6  col-md-6  col-sm-6 col-xs-12 col-lg-offset-3 col-md-offset-3 col-sm-offset-3">
							<form action="/feedback-post" method="POST">
										<div class="form-group">
											<label for="name">Name</label>
											<input type="text" class="form-control" id="name" placeholder="Enter your full name" name="name" required>
										</div>
										<div class="form-group">
											<label for="email">Email address</label>
											<input type="email" class="form-control" id="email" placeholder="Enter email" name="email" required>
										</div>
								<div class="form-group">
									<label for="email">Message</label>
									<textarea class="form-control textarea" rows="3" name="Message" id="Message" placeholder="Message"></textarea>
								</div>


								<button type="submit" class="btn btn-primary">Submit</button>
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
	</section>
	<footer id="footer">
	    <% include common/footer %>
	</footer>
</body>
</html>