
<html>
<head>
    <% include ../common/head %>
    <!--additional JS and css-->
    <script src="/js/app/pages/cms_admin.js" type="text/javascript"></script>
</head>
<body>
<header id="header" class="header full-width transparent fixed header-small clearfix">
    <% include ../common/header %>
</header>

<section id="content_body">
    <div class="space-box">
        <div class="container-fluid">
            <form class="defaultform row">
                <div class="col-md-6 col-md-offset-3">
                    <div class="register-widget clearfix">
                        <div class="space-top-small"></div>
                        <div class="widget-title">
                            <h2>Sign in to CMS</h2>
                            <hr>
                            <div id="msg"></div>
                        </div><!-- end title -->
                        <div class="row">
                            <div class="form-group col-lg-12">
                                <label>Username</label>
                                <input type="text" id="username" placeholder="Enter username" value="edu_admin"  class="form-control">
                            </div>
                            <div class="form-group col-lg-12">
                                <label>Password</label>
                                <input type="password" id="password" name="password" placeholder="Enter password" value="T$mp1234"  class="form-control">
                            </div>
                            <div class="col-sm-12">
                                <button type="button" id="btnAdmin" class="btn btn-edushala">Login</button><br>
                            </div>
                        </div>
                        <div class="space-top-small"></div>
                    </div>
                </div>
            </form><!-- end row -->
        </div><!-- end container -->
    </div>

</section>

<footer id="footer">
    <% include ../common/footer %>
</footer>

</body>
</html>